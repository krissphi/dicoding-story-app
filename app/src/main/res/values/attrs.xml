<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButton">
        <attr name="buttonVariant" format="enum">
            <enum name="primary" value="0"/>
            <enum name="secondary" value="1"/>
        </attr>

        <attr name="disabledText" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomEditText">
        <attr name="formVariant" format="enum">
            <enum name="text" value="0"/>
            <enum name="email" value="1"/>
            <enum name="password" value="2"/>
        </attr>
    </declare-styleable>
</resources>